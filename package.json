{
  "name": "pkstatus",
  "version": "0.9.1",
  "description": "Display PluralKit systems and their current status",
  "productName": "PKStatus",
  "homepage": "https://github.com/z0w13/pkstatus",
  "author": "Zowie <git@zowie.cx>",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "dev": "quasar dev",
    "release": "vite-node contrib/release.ts",
    "build": "quasar build",
    "prepare": "husky",
    "test:unit": "vitest",
    "test:unit:ci": "vitest run"
  },
  "lint-staged": {
    "src/**/*": [
      "prettier --list-different"
    ],
    "src/**/*.{ts,js,vue}": [
      "eslint"
    ]
  },
  "dependencies": {
    "@lumenpink/vue3-fitty": "^0.1.4",
    "@quasar/extras": "^1.16.11",
    "@types/markdown-it": "^14.0.0",
    "axios": "^1.6.8",
    "bottleneck": "^2.19.5",
    "dayjs": "^1.11.10",
    "markdown-it": "^14.1.0",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "pkapi.js": "^7.1.0",
    "quasar": "^2.15.2",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@commander-js/extra-typings": "^12.0.1",
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@commitlint/types": "^19.0.3",
    "@quasar/app-vite": "2.0.0-beta.5",
    "@quasar/icongenie": "^4.0.0",
    "@quasar/quasar-app-extension-testing-unit-vitest": "^1.0.0",
    "@types/node": "^20.12.5",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "@vue/test-utils": "^2.4.5",
    "autoprefixer": "^10.4.19",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "conventional-changelog-cli": "^4.1.0",
    "electron": "^29.2.0",
    "electron-builder": "^24.3.0",
    "electron-packager": "npm:@electron/packager@^18.3.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.24.1",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "tmp-promise": "^3.0.3",
    "typescript": "^5.4.4",
    "vite-plugin-checker": "^0.6.4",
    "vitest": "^1.4.0",
    "vue-tsc": "^1.8.27",
    "workbox-build": "^7.0.0",
    "workbox-cacheable-response": "^7.0.0",
    "workbox-core": "^7.0.0",
    "workbox-expiration": "^7.0.0",
    "workbox-precaching": "^7.0.0",
    "workbox-routing": "^7.0.0",
    "workbox-strategies": "^7.0.0"
  },
  "engines": {
    "node": "^24 || ^22 || ^20 || ^18",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
